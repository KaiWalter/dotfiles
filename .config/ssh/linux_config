Host kw-devvm
  User kai
  HostName kw-devvm.swedencentral.cloudapp.azure.com
  IdentityAgent ~/.1password/agent.sock
  ForwardAgent yes
  RequestTTY yes
Host kw-devvm-tunnel
  User kai
  HostName kw-devvm.swedencentral.cloudapp.azure.com
  IdentityAgent ~/.1password/agent.sock
  LocalForward 5000 localhost:5000
  LocalForward 5200 localhost:5200
  ForwardAgent yes
  RequestTTY yes
